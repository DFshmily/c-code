#include<stdio.h>

int main()
{
	int ret = 0;
	int ch = 0;
	char password[20] = { 0 };
	printf("请输入密码：>");
	scanf("%s", password);
	//清除缓存区
	while ((ch = getchar()) != '\n')')
	{
		;
	}
	printf("请确认密码(Y/N)：");
	ret = getchar();
	if (ret == 'Y')
	{
		printf("密码正确\n");

	}
	else
	{
		printf("取消确认\n");
	}

	return 0;
}
