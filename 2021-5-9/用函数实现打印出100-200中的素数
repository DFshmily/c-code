//用函数实现打印出100-200中的素数
#include<stdio.h>

int is_prime(int n)//判断是不是素数的函数
{
	int j;
	for (j = 2;j <n;j++)
	{
		if (n % j == 0)
			return 0;
	}
	/*if (j == n)
	{
		return 1;
	}*/
	return 1;
}

int main()
{
	int 1;
	for (1 = 100;1 <= 200;1++)
	{
		if (1 == is_prime(1))
			printf("%d  \n", 1);
	}
	return 0;
}
